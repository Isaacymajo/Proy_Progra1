@model IEnumerable<Banco.Entities.Apertura>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Tarjeta.Contraseña)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CodigoSeguimiento)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fecha)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Tasa)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TipoTarjeta)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Saldo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaEntrega)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Tarjeta.Contraseña)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CodigoSeguimiento)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Fecha)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Tasa)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TipoTarjeta)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Saldo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaEntrega)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.TransaccionId }) |
            @Html.ActionLink("Details", "Details", new { id=item.TransaccionId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.TransaccionId })
        </td>
    </tr>
}

</table>
